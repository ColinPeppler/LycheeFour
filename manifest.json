{
  "name": "Lychee Extension",
  "version": "1.0",
  "description": "Feed your pets by avoiding impulsive purchases!",
	"browser_action": {
		"default_popup": "popup.html"
	},
  "manifest_version": 2,
  "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "contextMenus"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.amazon.ca/*"],
            "js": ["content.js"]
        }
    ]
}
